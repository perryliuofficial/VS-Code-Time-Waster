{
	"name": "vscode-time-waster",
	"displayName": "VS Code Time Waster",
	"description": "Waiting for your code to compile? Try these time wasters!",
	"version": "0.0.2",
	"publisher": "perry",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/perryliuofficial/VS-Code-Time-Waster"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^4.8.4"
	}
